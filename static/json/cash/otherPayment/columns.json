      "disabled": true
    },
    {
      "prop": "money_day_used,day_deact",
      "label": "日累计金额/日停用金额",
      "width": "200",
      "class": "tCent",
      "delimiter":"/",
      "type":"connect,divisionMoney",
      "disabled": true
    },
    {
      "prop": "creator",
      "label": "建立人",
      "class": "tCent",
      "width":"120",
      "disabled": true
