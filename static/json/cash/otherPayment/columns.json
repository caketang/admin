    {
      "prop": "created",
      "label": "建立时间",
      "sort": true,
      "sortType": "date",
      "class": "tCent",
      "width": "200",
      "disabled": true
    },
    {
      "prop": "levels",
      "label": "层级",
      "class": "tCent",
      "width": "200",
      "disabled": true
    },
    {
      "prop": "terminal",
      "label": "终端设备",
      "class": "tCent",
      "width": "200",
      "disabled": true
    },
    {
      "prop": "sort",
      "label": "排序",
      "class": "tCent",
      "width":"50",
       "sortRemarks": "前台实际展示时数字大的排在前面",
      "disabled": true
    },
    {
      "prop": "status",
      "label": "状态",
      "class": "tCent",
      "width":"120",
      "disabled": true,
      "filters":[{"text":"启用","value": "1"},{"text":"停用","value": "0"}]
    },
    {
      "label": "操作",
      "type": "button",
      "disabled": true,
      "width":"180",
      "class": "tCent",
      "btnGroup": [
        {
          "btnType": "text",
          "label": "停用",
          "fn": "doDisable",
          "prop": "status",
          "val": "enabled"
        },
        {
          "btnType": "text",
          "label": "启用",
          "fn": "doEnable",
          "prop": "status",
          "val": "disabled"
        },
        {
          "btnType": "text",
          "label": "存款状况",
          "fn": "doDepositStatus"
        },
        {
          "btnType": "text",
          "label": "修改",
          "fn": "doEdit"
        },
        {
          "btnType": "text",
          "label": "删除",
          "fn": "doDelete"
        }
      ]
    }
  ]
